@{
    Layout = "_Layout";
}

<div ng-controller="TicketsController as tickets" ng-init="tickets.getCurrency()">
    <div>
        <select name="currency" ng-model="tickets.currency"
                ng-options="currency.Code as currency.Symbol for currency in tickets.currencies" required>
        </select>
    </div>
    <div>
        Пункт отправления
    </div>
    <div>
        Пункт назначения
    </div>
    <div>
        Дата вылета
    </div>
    <div>
        Дата возвращения
    </div>
</div>